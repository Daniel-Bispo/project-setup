{
    "env": {
        "es2020": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "import/prefer-default-export": "off",
        "import/no-unresolved": "off",
        "no-console": "off",
        "class-methods-use-this": "off",
        "quotes": [
            "error",
            "single",
            {
                "avoidEscape": true
            }
        ]
    }
}